<!DOCTYPE html>
<html>
  <head>
    <title>MathTesting</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'bootstrap',  media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'style',  media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'main',  media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>

    <!--header-->
    <%= render 'static_pages/header' %>
    <!--header-->
    <div class="container">
    <%= yield %>
    </div>
    <!--footer-->
    <%= render 'static_pages/footer' %>
    <!--footer-->
  </body>
</html>
